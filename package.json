{
  "name": "pickk-server",
  "version": "0.1.33",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "set": "cp .env.dev .env && docker compose -f docker-compose.yml up",
    "wset": "copy .env.dev .env && docker compose -f docker-compose.yml up",
    "set:down": "docker-compose -f docker-compose.yml down --volumes && docker system prune",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --debug-check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cp .env.dev .env && nest start --watch",
    "start:wdev": "copy .env.dev .env && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "tsc": "tsc --noEmit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:cov": "jest --coverage",
    "jest:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "jest:e2e": "jest --config ./test/jest-e2e.json",
    "test": "npm run format && npm run lint && npm run build && npm run jest",
    "pretypeorm": "npm list ts-node -g || npm install ts-node -g",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "makemigrations": "npm run typeorm migration:generate --",
    "migrate": "npm run typeorm migration:run",
    "migrate:revert": "npm run typeorm migration:revert",
    "migrations": "npm run typeorm migration:show",
    "eb:prod": "npm install && npm run start:prod",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0",
    "graphql-upload": "^11.0.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.10.0",
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^8.0.4",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.4",
    "@nestjs/elasticsearch": "^8.0.0",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.1",
    "@nestjs/swagger": "^5.0.0",
    "@nestjs/typeorm": "^8.0.1",
    "@ntegral/nestjs-sentry": "^3.0.3",
    "@pickk/nestjs-sqs": "^1.0.3",
    "@pickk/pay": "^1.3.2",
    "@sentry/node": "^6.11.0",
    "@slack/webhook": "^6.0.0",
    "apollo-server-express": "^2.25.2",
    "apollo-server-types": "^0.9.0",
    "aws-sdk": "^2.869.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "cheerio": "^1.0.0-rc.10",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dayjs": "^1.10.4",
    "graphql": "^15.5.1",
    "graphql-parse-resolve-info": "^4.11.0",
    "graphql-tools": "^7.0.5",
    "graphql-upload": "^12.0.0",
    "inicis": "^1.0.3",
    "joi": "^17.4.0",
    "jwks-rsa": "^2.0.3",
    "mysql": "^2.18.1",
    "nest-sens": "^1.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cache-manager": "^3.4.0",
    "@types/cache-manager-redis-store": "^2.0.0",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.5.3",
    "@types/graphql-upload": "^8.0.6",
    "@types/jest": "^26.0.24",
    "@types/joi": "^17.2.3",
    "@types/node": "^14.14.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.0",
    "ts-jest": "^27.0.4",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@src/(.*)$": "<rootDir>/$1",
      "@auth/(.*)$": [
        "<rootDir>/auth/$1"
      ],
      "@batch/(.*)$": [
        "<rootDir>/batch/$1"
      ],
      "@config/(.*)$": [
        "<rootDir>/config/$1"
      ],
      "@common/(.*)$": [
        "<rootDir>/common/$1"
      ],
      "@jobs/(.*)$": [
        "<rootDir>/jobs/$1"
      ],
      "@providers/(.*)$": [
        "<rootDir>/providers/$1"
      ],
      "@queue/(.*)$": [
        "<rootDir>/queue/$1"
      ],
      "@templates/(.*)$": [
        "<rootDir>/templates/$1"
      ],
      "@admin/(.*)$": "<rootDir>/modules/admin/$1",
      "@mcommon/(.*)$": "<rootDir>/modules/common/$1",
      "@content/(.*)$": "<rootDir>/modules/content/$1",
      "@item/(.*)$": "<rootDir>/modules/item/$1",
      "@my/(.*)$": "<rootDir>/modules/my/$1",
      "@order/(.*)$": "<rootDir>/modules/order/$1",
      "@payment/(.*)$": "<rootDir>/modules/payment/$1",
      "@user/(.*)$": "<rootDir>/modules/user/$1"
    }
  },
  "commitlint": {
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "enhance",
          "refactor",
          "perf",
          "test",
          "build",
          "chore",
          "ci",
          "docs",
          "revert",
          "style"
        ]
      ]
    }
  }
}
